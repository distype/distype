{
  "name": "distype",
  "version": "1.0.0",
  "description": "A Discord library written in TypeScript for developers looking to interface directly with the Discord API with little to no abstractions.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "docs": "rimraf ./docs && typedoc",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prepublishOnly": "npm i --include=dev && npm run build"
  },
  "keywords": [
    "api",
    "bot",
    "client",
    "discord",
    "discord-api",
    "discord-bot",
    "discordapp",
    "distype"
  ],
  "homepage": "https://distype.br88c.dev",
  "bugs": {
    "url": "https://github.com/distype/distype/issues"
  },
  "license": "Apache-2.0",
  "author": "BR88C",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distype/distype.git"
  },
  "dependencies": {
    "@br88c/node-utils": "^1.4.0",
    "@types/ws": "^8.5.3",
    "discord-api-types": "^0.37.0",
    "undici": "^5.8.0",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@br88c/eslint-config": "^1.6.0",
    "@types/node": "^16.11.47",
    "eslint": "^8.20.0",
    "rimraf": "^3.0.2",
    "typedoc": "^0.23.9",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.0"
  },
  "private": false
}
