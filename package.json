{
  "name": "distype",
  "version": "0.14.0",
  "description": "A Discord library written in TypeScript for developers looking to interface directly with the Discord API with little to no abstractions.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "docs": "rm -rf ./docs && typedoc",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prepublishOnly": "npm i --also=dev && npm run build"
  },
  "keywords": [
    "api",
    "bot",
    "client",
    "discord",
    "discord-api",
    "discord-bot",
    "discordapp",
    "distype"
  ],
  "homepage": "https://distype.br88c.dev",
  "bugs": {
    "url": "https://github.com/distype/distype/issues"
  },
  "license": "Apache-2.0",
  "author": "BR88C",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distype/distype.git"
  },
  "dependencies": {
    "@br88c/node-utils": "^1.3.1",  
    "@types/ws": "^8.5.3",
    "discord-api-types": "^0.34.0",
    "undici": "^5.5.1",
    "ws": "^8.8.0"
  },
  "devDependencies": {
    "@br88c/eslint-config": "^1.5.0",
    "@types/node": "^16.11.39",
    "eslint": "^8.17.0",
    "typedoc": "0.23.0-beta.4",
    "typescript": "^4.7.3"
  },
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.0"
  },
  "private": false
}
