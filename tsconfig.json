{
  "include": ["./src/**/*"],
  "exclude": [],
  "compilerOptions": {
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "declaration": true,
    "esModuleInterop": true,
    "lib": ["ES2021"],
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "lf",
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist",
    "strict": true,
    "target": "ES2021"
  }
}
